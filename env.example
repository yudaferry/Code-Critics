# Example environment file for Code Critics
# Copy this file to `.env` and replace the placeholder values with your secrets

# GitHub Personal Access Token with appropriate repository scopes
GITHUB_TOKEN=your_personal_access_token_here

# Shared secret configured on the GitHub webhook
WEBHOOK_SECRET=your_webhook_secret_here

# AI provider API keys
GEMINI_API_KEY=your_gemini_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Local development server port (optional)
PORT=3000 